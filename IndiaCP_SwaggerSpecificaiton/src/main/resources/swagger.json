        },
        "/indiacpissue/cancelCP/{cpIssueId}/{cancellationReason}": {
            "post": {
                "summary": "Cancels a CP providing the reason for the cancellation",
                "description": "Cancels a CP providing the reason for the cancellation",
                "operationId": "cancelCP",
                "parameters": [
                    {
                        "name": "cpIssueId",
                        "in": "path",
                        "description": "CP Issue ID that uniquely identifies the CP to be cancelled",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "cancellationReason",
                        "in": "path",
                        "description": "Reason for cancellation",
                        "required": true,
                        "type": "string"
                    }
                ],
                "tags": [
                    "Issuer, Investor, IPA, Depository"
                ],
                "responses": {
                    "200": {
                        "description": "Returns a CP Issue"
                    },
                    "default": {
                        "description": "Unexpected error",
                        "schema": {
                            "$ref": "#/definitions/Error"
                        }
                    }
                }
            }
        },
        "/fetchAllCP/{cpProgramId}": {
            "get": {
                "summary": "Get CP Issues allotted under a given CP Program",
                "description": "This returns all the CP Issues under the umbrella CP Program identified by an Id provided by the call\n",
                "operationId": "fetchAllCP",
                "parameters": [
                    {
                        "name": "cpProgramId",
                        "in": "path",
                        "description": "CP Program ID that uniquely identifies the CP Program issued by the Issuer",
                        "required": true,
                        "type": "string"
                    }
                ],
                "tags": [
                    "Issuer", "Investor"
                ],
                "responses": {
                    "200": {
                        "description": "Returns all CP Issues under the umbrella CP Program",
                        "schema": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/IndiaCPIssue"
                            }
                        }
                    },
                    "default": {
                        "description": "Unexpected error",
                        "schema": {
                            "$ref": "#/definitions/Error"
                        }
                    }
                }
            }
        },
        "/fetchAllCP": {
            "get": {
                "summary": "Get All Open CP Issues for the given Issuer/Investor. Open CP Issues refers to the Issues that are yet to mature",
                "description": "This returns all the Open CP Issues for the given DL Node",
                "operationId": "fetchAllCPOnThisNode",
                "parameters": [
                ],
                "tags": [
                    "Issuer", "Investor"
                ],
                "responses": {
                    "200": {
                        "description": "Get All Open CP Issues for the given Issuer/Investor. Open CP Issues refers to the Issues that are yet to mature",
                        "schema": {
