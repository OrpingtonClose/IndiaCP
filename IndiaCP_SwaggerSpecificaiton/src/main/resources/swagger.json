                    {
                        "name": "settlementDetails",
                        "in": "body",
                        "description": "Settlement Details",
                        "required": true,
                        "schema":
                        {
                            "$ref" : "#/definitions/SettlementDetails"
                        }
                    }

                ],
                "tags": [
                    "Issuer, Investor, IPA"
                ],
                "responses": {
                    "200": {
                        "description": "Amended the CP Issue with the Settlement Details",
                        "schema": {
                            "$ref": "#/definitions/IndiaCPIssue"
                        }
                    },
                    "default": {
                        "description": "Unexpected error",
                        "schema": {
                            "$ref": "#/definitions/Error"
                        }
                    }
                }
            }
        },
        "/indiacpissue/addDocs/{cpIssueId}": {
            "post": {
